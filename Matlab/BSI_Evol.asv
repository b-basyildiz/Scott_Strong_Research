function C_out = BSI_Evol(C)
%BSI_EVOL generates the next timestep of a curve. 
eps = 0.05; %This is ad hoc
Delta_C = zeros(size(C))
for i = 1:length(C)
        %Delta_C(:,i) = V_Evol(C,T,C(:,i),s,i,e); 
        Delta_C(:,i) = V_Evol(C,C(:,i),Derivative(C),i); 
    end
    C_out = C + eps*Delta_C; 
    
    function V = V_Evol(C,T,x,i)
        s = 1:length(C);
        x
        V =Biot_Savart(C,T,x,s); 
    end
end

